{
  "name": "coronavirus_tracker",
  "version": "1.0.0",
  "description": "simple app that keep tracking corona virus reports",
  "main": "server.js",
  "scripts": {
    "client": "npm start --prefix client",
    "start": "node -r esm server.js",
    "server": "nodemon -r esm server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rooneyrulz/coronavirus_tracker.git"
  },
  "keywords": [
    "rooney@ctracker"
  ],
  "author": "rooney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rooneyrulz/coronavirus_tracker/issues"
  },
  "homepage": "https://github.com/rooneyrulz/coronavirus_tracker#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "config": "^3.3.0",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.3",
    "morgan": "^1.9.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-wesbos": "0.0.19",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.3.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.16.4"
  },
  "engines": {
    "node": "13.2.0"
  }
}
